script

mkdir HW1

cd hw1

cat test.txt
>test.txt
git init

git add .

git status

git branch -m hw1

git status

git commit -m "Комичу пустой файл на ветку hw1"

git branch new

git status

git checkout new

echo "Вот какой-нибудь текст в файл test.txt" > test.txt

git status

git add .

git commit -m "Добавил ветку new и какой-нибудь текст в файл test.txt"

git chekout hw1

git branch second

git push

git push --set-upstream origin hw1

cp .bush_history E_OK_Python-QA

cd E_OK_Python-QA

git status

git add .

git status

exit
